{
	"name": "bf-react-forms",
	"homepage": "https://bfugina.github.io/react-form-builder-renderer",
	"version": "1.0.0",
	"description": "React form builder and renderer components",
	"main": "lib/index.js",
	"module": "lib/index.es.js",
	"jsnext:main": "lib/index.es.js",
	"types": "lib/index.d.ts",
	"files": [
		"lib/**/*"
	],
	"scripts": {
		"build": "yarn test && yarn build:only",
		"build:demo": "shx rm -rf build && yarn webpack --mode production --config webpack.demo.js",
		"build:master": "git checkout master && git fetch && git pull && yarn run build",
		"build:only": "shx rm -rf lib && rollup -c",
		"deploy:demo": "gh-pages -d build",
		"deploy:npm": "yarn deploy:npm:patch",
		"deploy:npm:patch": "yarn build:master && yarn version --patch && yarn publish:npm",
		"deploy:npm:minor": "yarn build:master && yarn version --minor && yarn publish:npm",
		"dev": "cross-env FORMS_BR_ENV=dev webpack-dev-server --open --config webpack.dev.js",
		"dev:log": "webpack-dev-server --open --config webpack.dev.js",
		"jest": "node node_modules/jest/bin/jest.js",
		"patch": "npm version patch",
		"predeploy:demo": "yarn test && yarn build:demo",
		"prettier": "prettier \"src/**/*.ts\" \"src/**/*.tsx\" \"src/**/*.js\" --write",
		"publish:npm": "yarn publish --non-interactive && git push",
		"rollup": "rollup -c",
		"rollup:watch": "rollup -c -w",
		"test": "yarn test:lint && yarn test:prettier && yarn test:unit",
		"test:lint": "tslint -p tsconfig.json",
		"test:prettier": "prettier \"src/**/*.ts\" \"src/**/*.tsx\" \"src/**/*.js\" --list-different",
		"test:unit": "cross-env TEST_ENV=unit yarn jest --config jest-unit.js",
		"webpack": "node node_modules/webpack/bin/webpack.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/BillFugina/react-form-builder-renderer.git"
	},
	"author": "Bill Fugina",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/BillFugina/react-form-builder-renderer/issues"
	},
	"devDependencies": {
		"@testing-library/jest-dom": "5.0.0",
		"@testing-library/react": "9.4.0",
		"@testing-library/react-hooks": "3.2.1",
		"@types/faker": "4.1.9",
		"@types/jest": "24.9.0",
		"@types/mockdate": "2.0.0",
		"@types/react": "16.9.17",
		"@types/react-dom": "16.9.4",
		"@types/react-test-renderer": "16.9.1",
		"@types/styled-components": "4.4.2",
		"@types/uuid": "3.4.6",
		"awesome-typescript-loader": "5.2.1",
		"copy-webpack-plugin": "5.1.1",
		"cross-env": "6.0.3",
		"css-loader": "3.4.2",
		"faker": "4.1.0",
		"gh-pages": "2.2.0",
		"html-webpack-plugin": "3.2.0",
		"identity-obj-proxy": "3.0.0",
		"jest": "24.9.0",
		"mockdate": "2.0.5",
		"path": "0.12.7",
		"prettier": "1.19.1",
		"react": "16.12.0",
		"react-dom": "16.12.0",
		"react-test-renderer": "16.12.0",
		"retoggle": "0.4.0",
		"rollup": "1.29.0",
		"rollup-plugin-commonjs": "10.1.0",
		"rollup-plugin-node-resolve": "5.2.0",
		"rollup-plugin-peer-deps-external": "2.2.0",
		"rollup-plugin-typescript2": "0.25.3",
		"shx": "0.3.2",
		"style-loader": "1.1.3",
		"styled-components": "5.0.0",
		"symlink-dir": "3.1.1",
		"ts-jest": "24.3.0",
		"ts-node": "8.6.2",
		"tsconfig-paths": "3.9.0",
		"tsconfig-paths-webpack-plugin": "3.2.0",
		"tslint": "5.20.1",
		"tslint-react-hooks": "2.2.1",
		"typescript": "3.7.5",
		"uglifyjs-webpack-plugin": "2.2.0",
		"uuid": "3.4.0",
		"webpack": "4.41.5",
		"webpack-cli": "3.3.10",
		"webpack-dev-server": "3.10.1",
		"webpack-merge": "4.2.2"
	},
	"peerDependencies": {
		"react": "^16.0.0",
		"react-dom": "^16.0.0",
		"styled-components": ">= 4"
	},
	"dependencies": {
		"date-fns": "2.9.0"
	}
}
