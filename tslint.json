{
  "extends": ["tslint-react-hooks"],
  "rules": {
    "no-console": {
      "severity": "warning"
    },
    "no-this-assignment": [
      true,
      {
        "allow-destructuring": true
      }
    ],
    "variable-name": false,

    // ===================================
    // Fix
    // ===================================
    "align": false,
    "array-type": false,
    "await-promise": false,
    "ban-comma-operator": false,
    "ban-types": false,
    "binary-expression-operand-order": false,
    "callable-types": false,
    "chai-vague-errors": false,
    "curly": false,
    "cyclomatic-complexity": false,
    "export-name": false,
    "forin": false,
    "function-name": false,
    "import-name": false,
    "informative-docs": false,
    "insecure-random": false,
    "interface-over-type-literal": false,
    "jsdoc-format": false,
    "jsx-key": false,
    "jsx-no-multiline-js": false,
    "match-default-export-name": false,
    "max-classes-per-file": false,
    "max-func-body-length": false,
    "max-line-length": false,
    "member-access": false,
    "member-ordering": false,
    "mocha-no-side-effect-code": false,
    "newline-before-return": false,
    "newline-per-chained-call": false,
    "no-angle-bracket-type-assertion": false,
    "no-any": false,
    "no-backbone-get-set-outside-model": false,
    "no-bitwise": false,
    "no-default-export": false,
    "no-duplicate-imports": false,
    "no-dynamic-delete": false,
    "no-empty-interface": false,
    "no-empty": false,
    "no-floating-promises": false,
    "no-for-in-array": false,
    "no-for-in": false,
    "no-function-expression": false,
    "no-http-string": false,
    "no-increment-decrement": false,
    "no-implicit-dependencies": false,
    "no-import-side-effect": false,
    "no-invalid-this": false,
    "no-namespace": false,
    "no-null-keyword": false,
    "no-object-literal-type-assertion": false,
    "no-parameter-properties": false,
    "no-parameter-reassignment": false,
    "no-redundant-jsdoc": false,
    "no-relative-imports": false,
    "no-require-imports": false,
    "no-reserved-keywords": false,
    "no-return-await": false,
    "no-shadowed-variable": false,
    "no-single-line-block-comment": false,
    "no-spread-object-literal-as-props": false,
    "no-string-literal": false,
    "no-string-throw": false,
    "no-submodule-imports": false,
    "no-suspicious-comment": false,
    "no-unnecessary-callback-wrapper": false,
    "no-unnecessary-class": false,
    "no-unnecessary-field-initialization": false,
    "no-unnecessary-initializer": false,
    "no-unnecessary-local-variable": false,
    "no-unnecessary-type-assertion": false,
    "no-unsafe-any": false,
    "no-unused-expression": false,
    "no-use-before-declare": false,
    "no-useless-files": false,
    "no-var-requires": false,
    "no-void-expression": false,
    "non-literal-fs-path": false,
    "object-literal-sort-keys": false,
    "only-arrow-functions": false,
    "prefer-cast-type": false,
    "prefer-method-signature": false,
    "prefer-object-spread": false,
    "prefer-readonly": false,
    "prefer-template": false,
    "prefer-type-cast": false,
    "promise-function-async": false,
    "react-a11y-anchors": false,
    "react-a11y-img-has-alt": false,
    "react-a11y-event-has-role": false,
    "react-a11y-input-elements": false,
    "react-a11y-no-onchange": false,
    "react-hooks-nesting": "error",
    "react-iframe-missing-sandbox": false,
    "react-no-dangerous-html": false,
    "react-this-binding-issue": false,
    "restrict-plus-operands": false,
    "strict-boolean-expressions": false,
    "switch-default": false,
    "switch-final-break": false,
    "type-literal-delimiter": false,
    "typedef": false,
    "use-default-type-parameter": false,
    "use-simple-attributes": false,
    "whitespace": false
  }
}
